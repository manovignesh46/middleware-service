    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nest-aws-sdk": "^3.0.1",
    "node-rsa": "^1.1.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "string-similarity-js": "^2.1.4",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.14",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.10",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "jest-mock-extended": "^3.0.4",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/src/main.ts",
      "/src/customers.module.ts",
      "src/infrastructure/controllers/common/errors",
      "src/infrastructure/repository",
      "/src/db/migrations",
      "/src/config",
      "/src/modules/error-mapping/mocks",
      "/src/modules/error-mapping/integrator-error-mapping.repository.ts"
    ],
    "setupFiles": [
      "../test/setup-jest.ts"
    ]
  },
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "keywords": []
}